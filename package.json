{
	"name": "lukbot",
	"version": "2.0.0",
	"description": "Discord bot for music playback and downloads",
	"main": "dist/index.js",
	"type": "module",
	"scripts": {
		"build": "tsc",
		"start": "tsx src/index.ts",
		"dev": "tsx watch src/index.ts",
		"lint": "eslint . -c eslint.config.js",
		"lint:fix": "eslint . -c eslint.config.js --fix",
		"type:check": "tsc --noEmit"
	},
	"keywords": [
		"discord",
		"bot",
		"music",
		"typescript"
	],
	"author": "",
	"license": "ISC",
	"dependencies": {
		"@discordjs/builders": "^1.7.0",
		"@discordjs/opus": "^0.10.0",
		"@discordjs/rest": "^2.2.0",
		"@discordjs/voice": "^0.16.1",
		"bgutils-js": "^3.2.0",
		"chalk": "^5.3.0",
		"discord-player": "^6.1.1",
		"discord-player-youtubei": "^1.4.5",
		"discord.js": "^14.14.1",
		"dotenv": "^16.4.5",
		"ffmpeg-static": "^5.2.0",
		"fluent-ffmpeg": "^2.1.3",
		"module-alias": "^2.2.3",
		"opusscript": "^0.0.8",
		"play-dl": "^1.9.7",
		"sodium-native": "^5.0.1"
	},
	"devDependencies": {
		"@types/fluent-ffmpeg": "^2.1.27",
		"@types/node": "^20.11.19",
		"@typescript-eslint/eslint-plugin": "^7.0.1",
		"@typescript-eslint/parser": "^7.0.1",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"glob": "^10.3.10",
		"nodemon": "^3.1.10",
		"prettier": "^3.2.5",
		"ts-node": "^10.9.2",
		"ts-node-dev": "^2.0.0",
		"tsconfig-paths": "^4.2.0",
		"tsx": "^4.19.3",
		"typescript": "^5.3.3"
	},
	"_moduleAliases": {
		"@": "dist",
		"@models": "dist/models",
		"@utils": "dist/utils",
		"@handlers": "dist/handlers",
		"@functions": "dist/functions",
		"@config": "dist/config",
		"@types": "dist/types",
		"@commands": "dist/commands",
		"@events": "dist/events"
	}
}
